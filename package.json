{
  "name": "sack-gui",
  "version": "0.9.123",
  "description": "SACK abstraction library exposed to JS to provide low level system services including GUI.",
  "files": [
	"package.json",
	"build/Release",
	"build/Debug",
	"apps",
	"import.mjs",
	"vfs_module.cjs",
	"vfs_module.mjs",
	"sack-jsox.cjs"
  ],
  "repository": {
      "type": "git",
      "url": "https://github.com/d3x0r/sack.vfs/",
      "branch": "sack-gui"
  },
  "main": "vfs_module.cjs",
  "module": "vfs_module.mjs",
  "vm": "vfs_ivm.mjs",
  "type": "commonjs",
  "exports": {
    ".": {
      "require": "./vfs_module.cjs",
      "import": "./vfs_module.mjs"
    },
    "./sack": {
      "require": "./vfs_module.cjs",
      "import": "./vfs_module.mjs"
    },
    "./import.mjs": {
      "default": "./import.mjs"
    },
    "./import": {
      "default": "./import.mjs"
    },
    "./object-storage": {
      "default": "./apps/object-storage/object-storage.mjs"
    },
    "./object-storage-object": {
      "default": "./apps/object-storage/object-storage-object.mjs"
    },
    "./apps/http-ws": {
      "require": "./apps/http-ws/server.js",
      "import": "./apps/http-ws/server.mjs"
    },
    "./http-ws": {
      "require": "./apps/http-ws/server.js",
      "import": "./apps/http-ws/server.mjs"
    },
    "./protocol": {
      "import": "./apps/http-ws/server-protocol.mjs"
    },
    "./server-protocol": {
      "import": "./apps/http-ws/server-protocol.mjs"
    },
    "./client-protocol": {
      "require": "./apps/http-ws/client-node-protocol.js",
      "import": "./apps/http-ws/client-node-protocol.mjs"
    },
    "./web-protocol": {
      "default": "./apps/http-ws/client-protocol.js"
    },
    "./apps/http-ws/uexpress": {
      "require": "./apps/http-ws/uexpress.js",
      "import": "./apps/http-ws/uexpress.mjs"
    },
    "./net-import": {
      "require": "./apps/net-import/net-import.js",
      "import": "./apps/net-import/net-import.mjs"
    },
    "./uexpress": {
      "require": "./apps/http-ws/uexpress.js",
      "import": "./apps/http-ws/uexpress.mjs"
    },
    "./Events": {
      "require": "./apps/events/events.js",
      "import": "./apps/events/events.mjs"
    },
    "./Events2": {
      "import": "./apps/events/events2.mjs"
    },
    "./prng": {
      "require": "./apps/prng/prng_short.js",
      "import": "./apps/prng/prng_short.mjs"
    },
    "./task-manager": {
      "import": "./apps/task-manager/src/main.mjs"
    },
    "./dbUtil": {
      "import": "./apps/dbUtil/db.mjs"
    },
    "./isTopLevel": {
      "import": "./apps/isTopLevel/isTopLevel.mjs"
    },
    "./text": {
      "import": "./apps/text/text.mjs"
    },
    "./wait": {
      "import": "./apps/wait/wait.mjs"
    },
    "./slab-array": {
      "import": "./apps/slab-array/SlabArray.mjs"
    },
    "./bloom-n-hash": {
      "import": "./apps/bloom-n-hash/bloomNHash.mjs"
    },
    "./bloomnhash": {
      "import": "./apps/bloom-n-hash/bloomNHash.mjs"
    }
  },
  "private": false,
  "preferGlobal": false,
  "license": "LGPL",
  "keywords": [
    "sqlite",
    "vfs",
    "JSON",
    "JSON6",
    "GUI",
    "Images",
    "jpeg",
    "png",
    "jpg",
    "opengl",
    "vulkan",
    "windows",
    "controls",
    "memory mapped",
    "encrypt",
    "decrypt",
    "configuration",
    "options",
    "windows registry",
    "database",
    "sack"
  ],
  "scripts": {
    "test": "node tests/vfsTest.js",
    "install": "npm run build-gui",
    "build-gui": "npm run build-gui-config && npm run build-gui-run1 && npm run build-gui-config && npm run build-gui-run2",
    "build-gui-config": "npx cmake-js --CDMAKE_GUI=1 --CDCMAKE_BUILD_TYPE=Release configure",
    "build-gui-run1": "npx cmake-js build",
    "build-gui-run2": "npx cmake-js build",
    "build-gui-debug": "npm run build-gui-debug-config && npm run build-gui-debug-run1 && npm run build-gui-debug-config && npm run build-gui-debug-run2",
    "build-gui-debug-config": "npx cmake-js --CDMAKE_GUI=1 --CDCMAKE_BUILD_TYPE=Debug -D configure",
    "build-gui-debug-run1": "npx cmake-js build -D",
    "build-gui-debug-run2": "npx cmake-js build -D",

    "build-gui-reldeb": "npm run build-gui-reldeb-config && npm run build-gui-reldeb-run1 && npm run build-gui-reldeb-config && npm run build-gui-reldeb-run2",
    "build-gui-reldeb-config": "npx cmake-js --CDMAKE_GUI=1 --CDCMAKE_BUILD_TYPE=RelWithDebInfo -D configure",
    "build-gui-reldeb-run1": "npx cmake-js build -D --CDCMAKE_BUILD_TYPE=RelWithDebInfo",
    "build-gui-reldeb-run2": "npx cmake-js build -D --CDCMAKE_BUILD_TYPE=RelWithDebInfo",

    "build-vfs": "npm run build-vfs-config && npm run build-vfs-run1 && npm run build-vfs-config && npm run build-vfs-run2",
    "build-vfs-config": "npx cmake-js --CDMAKE_GUI=0 --CDCMAKE_BUILD_TYPE=Release configure",
    "build-vfs-run1": "npx cmake-js build",

    "build-vfs-debug": "npm run build-vfs-debug-config && npm run build-vfs-debug-run1",
    "build-vfs-debug-config": "npx cmake-js --CDMAKE_GUI=0 --CDCMAKE_BUILD_TYPE=Debug -D configure",
    "build-vfs-debug-run1": "npx cmake-js build -D",

    "build-vfs-reldeb": "npm run build-vfs-reldeb-config && npm run build-vfs-reldeb-run1",
    "build-vfs-reldeb-config": "npx cmake-js --CDMAKE_GUI=0 --CDCMAKE_BUILD_TYPE=RelWithDebInfo configure",
    "build-vfs-reldeb-run1": "npx cmake-js build --CDCMAKE_BUILD_TYPE=RelWithDebInfo",

    "build": "npm run build-gui"
  },
  "devDependencies": {
    "@babel/preset-env": "latest",
    "@d3x0r/bloomnhash": "latest",
    "@d3x0r/object-storage": "latest",
    "@d3x0r/popups": "latest",
    "@d3x0r/slab-array": "latest",
    "@d3x0r/srg": "latest",
    "@rollup/plugin-babel": "latest",
    "@rollup/plugin-commonjs": "latest",
    "@rollup/plugin-node-resolve": "^13.0.0",
    "@rollup/plugin-strip": "latest",
    "@rollup/plugin-terser": "latest",
    "acorn": "latest",
    "chai": "latest",
    "cmake-js": "latest",
    "core-js": "latest",
    "eslint": "latest",
    "husky": "latest",
    "jsox": "latest",
    "lint-staged": "latest",
    "mocha": "latest",
    "nyc": "latest",
    "rollup": "latest"
  },
  "nyc": {
    "statements": 100,
    "branches": 100,
    "functions": 100,
    "lines": 100,
    "ignore-class-method": [
      "log"
    ],
    "reporter": [
      "lcov",
      "text"
    ]
  }
}
